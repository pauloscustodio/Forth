".", 			DOT, 			0, 		cout << vm.pop() << VM::BL
"THROW", 		THROW, 			0, 		error(static_cast<Error>(vm.pop()))
"DROP", 		DROP, 			0, 		vm.pop()
"DUP", 			DUP, 			0, 		vm.push(vm.peek(0))
"PICK", 		PICK, 			0, 		vm.push(vm.peek(vm.pop()))
"+", 			PLUS, 			0, 		vm.push(vm.pop()+vm.pop())
".S", 			DOT_S, 			0, 		vm.dot_stack()
"WORDS", 		WORDS, 			0, 		vm.words()
"S""",			S_QUOTE,		0,		vm.f_s_quote()
"TYPE",			TYPE,			0,		vm.f_type()
"ENVIRONMENT?", ENVIRONMENT_Q, 	0, 		vm.f_environment_q()
